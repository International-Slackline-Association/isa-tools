{
  "name": "isa-tools-server",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "checkTs": "tsc --noEmit",
    "build": "tsc --build",
    "package": "serverless package",
    "deploy": "eslint . && npm run build && serverless deploy --verbose",
    "deploy:api": "serverless deploy function -f api",
    "deploy:imageProcessorApi": "serverless deploy function -f imageProcessorApi",
    "deploy:prerender": "aws cloudformation deploy --template-file misc/prerender-cloudformation.yaml --stack-name Prerender --capabilities CAPABILITY_IAM --region=us-east-1 --parameter-overrides PrerenderToken=fE4XxQzGelPlja0KVTna",
    "invoke:certificateTester": "serverless invoke local -f certificateTester --verbose"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "3.478.0",
    "@aws-sdk/client-dynamodb": "3.478.0",
    "@aws-sdk/client-s3": "3.478.0",
    "@aws-sdk/client-ssm": "3.478.0",
    "@aws-sdk/lib-dynamodb": "3.478.0",
    "@aws-sdk/s3-request-presigner": "3.481.0",
    "@pdf-lib/fontkit": "1.1.1",
    "@vendia/serverless-express": "4.12.6",
    "axios": "1.6.2",
    "cors": "2.8.5",
    "date-fns": "3.0.6",
    "express": "4.18.2",
    "googleapis": "129.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash.clonedeep": "4.5.0",
    "pdf-lib": "1.17.1",
    "qrcode": "1.5.3",
    "winston": "3.11.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@serverless/typescript": "3.38.0",
    "@types/aws-lambda": "8.10.130",
    "@types/cors": "2.8.17",
    "@types/dateformat": "5.0.2",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/lodash.clonedeep": "4.5.9",
    "@types/node": "20.10.5",
    "@types/qrcode": "1.5.5",
    "esbuild": "0.19.10",
